/* modules/reportpage/mddata/md-data.wxss */

@import "/resources/wxss/layout.wxss";
@import "/resources/wxss/base/display.wxss";
@import "/resources/wxss/base/clear.wxss";
@import "/resources/wxss/iconfont/iconfont.wxss";
@import "/resources/wxss/base/icon.wxss";

.md-data-info {
  width: 690rpx;
  height: 76rpx;
  font-size: 26rpx;
  line-height: 76rpx;

  padding: 0 20rpx;
  margin: 0 auto;
}

.m-d-i-text-r {
  float: right;
  color: #818181;
}

.md-data-view {
  width: 710rpx;
  padding: 10rpx;
  margin: 0 auto;

  background: #fff;
  border-radius: 15rpx;
}
.md-data-table {
  width: 690rpx;
  margin: 0 auto;
}
.m-d-t-c1 {
  width: 250rpx;
}
.m-d-img {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
}
.m-d-img-l {
  margin-right: 10rpx; 
}
.m-d-img-r {
  margin-left: 10rpx; 
}
.m-d-text {
  font-size: 26rpx;
  line-height: 50rpx;
}
.m-d-t-sub {
  font-size: 22rpx;
  color: #aaa;
  padding: 0 10rpx;
}

.md-data-table-view {
  width: 690rpx;
  padding: 10rpx;
  margin-top: 10rpx;

  background: #f8f9fe;
  border-radius: 15rpx;
}
.m-d-t-text {
  font-size: 22rpx;
  color: #aaa;
}
.md-data-progress-view {
  margin: 5rpx 0 10rpx 0;
}


.md-data-view-blue {
  width: 730rpx;
  margin: 0 auto;

  background: #fff;
  border-radius: 15rpx;
}
.m-d-v-b-data {
  margin-bottom: 20rpx;
}
.m-d-b-title {
  width: 730rpx;
  font-size: 24rpx;
  color: #fff;

  text-align: center;
  line-height: 50rpx;

  background: #fea21b;
  border-radius: 15rpx 15rpx 0 0;
}
.m-d-b-c1 {
  width: 140rpx;
}
.m-d-b-c3 {
  width: 450rpx;
}
.m-d-b-c4 {
  width: 180rpx;
}
.m-d-b-c5 {
  width: 100rpx;
}
.m-d-b-line {
  width: 730rpx;
  text-align: center;

  padding: 20rpx 0;
}
.line-border{
  border-bottom: solid 2rpx #ddd;
}
.m-d-b-time {
  font-size: 22rpx;
  line-height: 30rpx;
  color: #666;
}

.m-d-logo1 {
  width: 80rpx;
  height: 45rpx;
  vertical-align: middle;
}
.m-d-logo2 {
  width: 45rpx;
  height: 45rpx;
  vertical-align: middle;
}
.m-d-win-view {
  width: 100%;
  height: 40rpx;
  position: relative;
}
.m-d-win {
  position: absolute;
  top: -14rpx;
}
.m-d-win-l {
  left: 30rpx;
}
.m-d-win-r {
  right: 30rpx;
}


.m-d-report {
  width: 670rpx;
  padding: 0 30rpx;
}
.m-d-r-c1 {
  width: 160rpx;
}
.m-d-r-c2 {
  width: 100rpx;
  text-align: center;

  font-size: 46rpx;
  vertical-align: top;
}
.m-d-r-c3 {
  width: 150rpx;
  text-align: center;
}
.m-d-r-num {
  font-size: 34rpx;
}
.m-d-r-time {
  font-size: 22rpx;
  color: #666;
  padding-left: 10rpx;
  letter-spacing: 10rpx;
  background: #eee;
}
.m-d-r-item {
  font-size: 24rpx;
  line-height: 30rpx;
  color: #ed5e46;
  text-align: center;
}
.m-d-r-il {
  float: left;
  margin-right: 10rpx;
}
.m-d-r-ir {
  float: right;
  margin-left: 10rpx;
}

.m-d-r-name {
  font-size: 24rpx;
  color: #666;
}

.m-d-picture {
  width: 670rpx;
  padding: 0 30rpx 20rpx 30rpx;
}
.m-d-p-c1 {
  width: 240rpx;
}
.m-d-p-c2 {
  text-align: center;
}
.m-d-p-pic {
  width: 45rpx;
  height: 75rpx;
}
.pic-l {
  float: left;
  margin-right: 2rpx;
}
.pic-r {
  float: right;
  margin-left: 2rpx;
}
.m-d-p-check-v {
  padding-top: 40rpx;
}
.m-d-p-check {
  display: inline-block;
  width: 190rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;

  color: #fff;
  font-size: 26rpx;
  
  background: #292c2d;
  border-radius: 20rpx;
}



