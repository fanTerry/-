<!--component/report/reportbanner/report-banner.wxml-->
<view class='report-banner'>
  <swiper class="report-banner-swiper"
    indicator-dots="true" autoplay="true" circular="true" bindchange="onSwiperChange">
    <block wx:for="abc" wx:key="">
      <swiper-item>
        <image class="r-b-swiper-image" src="https://rs.esportzoo.com/svn/esport-res/mini/images/image4.png" mode="scaleToFill" />
      </swiper-item>
    </block>
  </swiper>
</view>