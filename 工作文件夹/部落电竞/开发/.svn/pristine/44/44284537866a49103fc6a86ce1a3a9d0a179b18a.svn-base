<!--component/reportlist/reportexpertitem/report-expert-item.wxml-->

<view class='report-expert-item'>

  <!-- 第一部分 -->
  <view class='report-expert-info dsp-table'>
    
    <view class="dsp-cell report-expert-avatar">
      <navigator url='/modules/expertpage/expertapplypage/expert-apply-page'>
        <image class="userinfo-avatar" src="/resources/images/avatar/cat.jpg" mode="aspectFill" />
      </navigator>
    </view>

    <view class='dsp-cell'>
      <view class='report-expert-info-detail'>
        <text class='r-e-name'>老毛精算</text>
        <i class='r-e-tag'>近7中6</i>
      </view>
      <view class='report-expert-info-note'>
        <text>推荐买点：</text>
        <view class='r-e-high'>
          <text class='r-e-h-text'>胜负让分</text>
          <text class='r-e-h-text'>大小分</text>
          <text class='r-e-h-text'>十二杀</text>
        </view>
      </view>
    </view>

    <view class='dsp-cell'>
      <view class='report-export-num'>66%</view>
      <view class='report-export-num'>近期回报率</view>
    </view>
    
  </view>

  <!-- 第二部分 -->
  <view class='report-expert-detail dsp-table'>
    
    <view class='report-expert-detail-vs dsp-cell '>
      <view class='r-e-d-tit-view'>
        <i class='iconfont icon-shoucang r-e-d-logo'></i>
        <text class='r-e-d-tit'>MDL 巴黎迪士尼Major预选赛巴黎迪士尼Major预选赛巴黎迪士尼Major预选赛 BO3</text>
      </view>

      <view wx:for="ab" class='dsp-table'>
        <view class='dsp-cell'>
          <text class='r-e-d-text t-r'>DETON...</text>
        </view>
        
        <view class='dsp-cell'>
          <image class='r-e-d-img' src='/resources/images/image1.png' mode='aspectFill' />
        </view>

        <view class='dsp-cell'>
          <text class='r-e-d-vs'>VS</text>
        </view>

        <view class='dsp-cell'>
          <image class='r-e-d-img' src='/resources/images/image1.png' mode='aspectFill' />
        </view>

        <view class='dsp-cell'>
          <text class='r-e-d-text t-l'>DETON...</text>
        </view>
      </view>
    </view>

    <view class='report-expert-detail-money'>
      <view>
        <i class='iconfont icon-xiaojinbi icon-dollar'></i>
        <text class='r-e-d-money'>800.0022竞彩币</text>
      </view>
      <view>
        <i class='r-e-d-note'>不中全退</i>
      </view>
    </view>

  </view>

  <view class='report-expert-sub'>
    <text>1小时前发布 | 阅读 18</text>
  </view>

</view>