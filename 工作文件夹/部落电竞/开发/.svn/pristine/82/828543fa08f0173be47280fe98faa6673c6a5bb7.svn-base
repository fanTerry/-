<!--modules/store/list/goodsList.wxml-->
<view class="mod_header" wx:if="{{pageType==2}}">
	<submenu-bar titleArray="{{categoryList}}" selectedTag="{{selectedTag}}" bindmyevent="onMyEvent"></submenu-bar>
</view>

<view class="main">
	 <goods-list myClass='{{myClass}}' listGoods='{{shopGoodList}}' type='{{pageType}}' band='{{goodsQueryType}}'></goods-list>
</view>
