<script>
import Swiper from "swiper";
import bet from "../../components/guessbet/bet";
import pops from "../../components/pop/pops";
import modTitle from "../../components/title/title";
// import localStorage from "../../libs/storages/localStorage";
import sessionStorage from "../../libs/storages/sessionStorage";

export default {
  components: {
    bet,
    pops,
    modTitle
  },
  props: [],
  data() {
    return {
      userInfo: null,
      iconImg: null,
      sceneStr: null,
      qrcodeUrl: null,
      websock: null,
      loginType: 1,
      type: 0,
      showType: false,
      guessMarkNum: 0,
      popGuessMark: false,
      guessItem: ["猜MVP", "猜冠军", "猜英雄", "猜五杀"],
      voteMark: 0,
      matchMoreMark: Number
    };
  },

  mounted() {
    this.$nextTick(() => {
      this.initSwiper();
    });
    this.getUserInfo();
    this.clearLocalStorage();
  },
  methods: {
    initSwiper() {
      this.swiper = new Swiper(".swiper-container", {
        loop: true,
        speed: 2000,
        autoplay: true,
        slidesPerView: "auto",
        spaceBetween: 20,
        autoplayDisableOnInteraction: false,
        observer: true,
        observeParents: true
      });
    },
    login() {
      let date = null;
      this.$axios
        .get("/api/wxlogin/scanLogin", date)
        .then(successResponse => {
          this.qrcodeUrl = successResponse.data.qrcodeUrl;
          this.sceneStr = successResponse.data.sceneStr;
          this.initWebSocket();
          // console.log(this.qrcodeUrl);
          // console.log(this.sceneStr);
        })
        .catch(error => {
          console.log(error);
        });
    },

    initWebSocket() {
      if ("WebSocket" in window) {
        const wsuri = "ws://www.esportzoo.cn/websocket"; //ws地址
        this.websock = new WebSocket(wsuri);
        this.websock.onopen = this.websocketonopen;
        this.websock.onerror = this.websocketonerror;
        this.websock.onmessage = this.websocketonmessage;
        this.websock.onclose = this.websocketclose;
      } else {
        alert("当前浏览器无法获取二维码，请更换浏览器");
      }
    },
    websocketonopen() {
      console.log("WebSocket连接成功");
      // console.log(this.sceneStr);
      this.websock.send(this.sceneStr);
      this.type = 2;
      this.showType = true;
    },
    websocketonerror(e) {
      //错误
      console.log("WebSocket连接发生错误");
      this.login();
    },
    websocketonmessage(e) {
      //数据接收
      // console.log("数据开始接收");
      console.log(e.data); // 接收数据
      this.userInfo = JSON.parse(e.data);
      this.iconImg = this.userInfo.icon;
      sessionStorage.set("user", this.userInfo);

      console.log(this.userInfo.nickname);
      this.loginType = 0;
      this.type = 0;
      this.showType = false;
    },

    websocketclose(e) {
      //关闭
      console.log("WebSocket连接关闭", e);
    },

    // 公用关闭弹窗
    closePop() {
      this.type = 0;
      this.showType = false;
    },

    //获取本地用户信息
    getUserInfo() {
      console.log("初始化1");

      this.userInfo = sessionStorage.get("user");
      console.log(this.userInfo);
      if (this.userInfo == null) {
        console.log("没有获得用户信息");
      } else {
        console.log("已获得用户信息");
        this.iconImg = this.userInfo.icon;
        this.loginType = 0;
        this.type = 0;
        this.showType = false;
      }
    },
    //清除缓存
    clearLocalStorage() {      
      /* window.onbeforeunload = function(e) {
        var storage = window.localStorage;
        storage.clear();
      }; */
    },

    //用户退出
    singout() {
      this.popGuessMark = false;
      this.iconImg = null;
      this.userInfo = null;
      this.loginType = 1;
      console.log(this.userInfo);
      sessionStorage.remove("user");
    },
    //微信登录
    wxLogin() {
      this.type = 1;
      this.showType = true;
    },
    openMore(index) {
      if (this.matchMoreMark == index) {
        this.matchMoreMark = -1;
      } else {
        this.matchMoreMark = index;
      }
    }
  }
};
</script>

