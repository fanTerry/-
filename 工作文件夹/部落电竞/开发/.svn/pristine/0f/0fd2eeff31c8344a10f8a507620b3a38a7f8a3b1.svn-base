package com.esportzoo.esport.connect.response.hd;

import java.io.Serializable;

public class HdSignResponse implements Serializable {

	private static final long serialVersionUID = 1L;
	
	/**
	 * 礼品ID
	 */
	private Integer hdGiftId;
	/**
	 * 礼品名称
	 */
	private String hdGiftName;
	/**
	 * 第几天
	 */
	private Integer signFlagBit;
	/**
	 * 礼品数量
	 */
	private Integer hdGiftCount;
	/**
	 * 是否已领取 0已领取 1未领取 2不能领取
	 */
	private Integer receiveStatus;
	/**
	 *第七天随机金豆标识
	 */
	private String sevenFlag;
	public Integer getHdGiftId() {
		return hdGiftId;
	}
	public void setHdGiftId(Integer hdGiftId) {
		this.hdGiftId = hdGiftId;
	}
	public Integer getSignFlagBit() {
		return signFlagBit;
	}
	public void setSignFlagBit(Integer signFlagBit) {
		this.signFlagBit = signFlagBit;
	}
	public Integer getHdGiftCount() {
		return hdGiftCount;
	}
	public void setHdGiftCount(Integer hdGiftCount) {
		this.hdGiftCount = hdGiftCount;
	}
	public String getHdGiftName() {
		return hdGiftName;
	}
	public void setHdGiftName(String hdGiftName) {
		this.hdGiftName = hdGiftName;
	}
	public String getSevenFlag() {
		return sevenFlag;
	}
	public void setSevenFlag(String sevenFlag) {
		this.sevenFlag = sevenFlag;
	}
	public Integer getReceiveStatus() {
		return receiveStatus;
	}
	public void setReceiveStatus(Integer receiveStatus) {
		this.receiveStatus = receiveStatus;
	}
	
}
