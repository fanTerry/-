.button{
  font-size: 40rpx;
  color: #fff;
}


.ui_pop {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  /* @extend .flex_v_h; */
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0, 0, 0, .7);
  z-index: 999;
}


.mod_pop {
  position: relative;
  width: 676rpx;
  border: 6rpx solid #333;
  border-radius: 86rpx;
  background-color: #fff;
}
.close {
  position: absolute;
  right: 0;
  top: 0;
  z-index: 99;
  width: 76rpx;
  height: 76rpx;
	background: url('https://daily-rs.esportzoo.com/svn/esport-res/mini/images/hd/hd101/close.png') no-repeat center;
  background-size: contain;
}
.share_tag {
  position: absolute;
  right: 40rpx;
  top: 0;
  transform: translateY(-100%);
  width: 86rpx;
  height: 216rpx;
	background: url('https://daily-rs.esportzoo.com/svn/esport-res/mini/images/hd/hd101/share_tag.png') no-repeat center;
  background-size: cover;
}
.title {
  padding: 100rpx 76rpx 0;
  font-size: 40rpx;
  line-height: 68rpx;
  text-align: center;
}
.rule_tips {
  margin-top: 220rpx;
  padding-bottom: 64rpx;
}

.rule_tips p {
  display: block;
	padding-top: 20rpx;
	padding-left: 90rpx;
	font-size: 30rpx;
}

.pop_share .rule_tips p {
  display: block;
  text-align: center;
  padding-left: 0;
}

.pop_nochance {
  padding: 200rpx 76rpx;
}

.pop_nochance .title {
	padding: 0;
}

.award_logo {
  position: absolute;
  top: 250rpx;
  right: 26rpx;
  width: 574rpx;
}

.pop_award {
  top: 132rpx;
}

.pop_award .close {
	/* top: 188rpx;
	transform: translateY(-100%); */
}
.pop_award .title {
	line-height: 1;
	text-align: center;
}

.pop_award .title:first-of-type {
	padding-top: 60rpx;
}
.pop_award .title:nth-of-type(3) {
	padding-top: 126rpx;
	margin-bottom: 30rpx;
}

.pop_award .award_title {
	/* @extend .flex_v_h; */
	display: flex;
	align-items: center;
	justify-content: center;
	padding-top: 48rpx;
	font-size: 96rpx;
	color: #e97542;
	text-align: center;
}

.pop_award .award_title span {
	font-size: 80rpx;
}

.pop_award .rule_tips {
	margin-top: 0;
}

.pay_one {
  padding-bottom: 16rpx;
  font-size: 30rpx;
}

.att_tips {
  font-size: 30rpx;
  text-align: center;
}

.money {
  font-size: 90rpx;
  color: #333;
  text-align: center;
}

.success_logo {
  display: block;
  width: 170rpx;
  height: 170rpx;
  margin: 88rpx auto 40rpx;
}

.count_tips {
	/* @extend .g_c_mid; */
	position: absolute;
	left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  width: 148rpx;
  height: 148rpx;
  line-height: 148rpx;
  border-radius: 50%;
  font-size: 90rpx;
  font-family: "PangMenZhengDao";
  color: #ffe783;
  text-align: center;
  background-color: #e97542;
  box-shadow: 0 10rpx 0 0 rgba(#000000, 0.12);
}

.bg_green,
.bg_red,
.bg_grey {
	/* @extend .flex_v_h; */
	display: flex;
	align-items: center;
	justify-content: center;
  flex-direction: column;
  -webkit-flex-direction: column;
  width: 570rpx;
  height: 126rpx;
  margin: 0 auto 20rpx;
  font-size: 40rpx;
  letter-spacing: 10rpx;
  color: #fff;
  background-color: initial;
}

.bg_green {
	background: url('https://daily-rs.esportzoo.com/svn/esport-res/mini/images/hd/hd101/bg_green.png') no-repeat center;
  background-size: cover;
}

.bg_red {
	background: url('https://daily-rs.esportzoo.com/svn/esport-res/mini/images/hd/hd101/bg_red.png') no-repeat center;
  background-size: cover;
}

.bg_grey {
	background: url('https://daily-rs.esportzoo.com/svn/esport-res/mini/images/hd/hd101/bg_grey.png') no-repeat center;
  background-size: cover;
}

.mt14 {
  margin-top: 28rpx;
}

.mb7 {
  margin-bottom: 14rpx;
}

.mt36 {
  margin-top: 72rpx;
}

.mb36 {
  margin-bottom: 72rpx;
}
.mb50 {
  margin-bottom: 100rpx;
}

.lh_one {
  line-height: 1;
}

.lh34 {
  line-height: 68rpx !important;
}

.tac {
  text-align: center;
}

.pl0 {
  padding-left: 0 !important;
}