package com.esportzoo.esport.vo.user;

import java.io.Serializable;

/**
 * @author tingting.shen
 * @date 2019/07/16
 */
public class ValidRegisterCodeRequest implements Serializable {

	private static final long serialVersionUID = -1154052773754296540L;
	
	//验证码类型 1注册验证码 2重置密码验证码
	private Integer type;
	private String phone;
	private String code;
	
	public Integer getType() {
		return type;
	}
	public String getPhone() {
		return phone;
	}
	public String getCode() {
		return code;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public void setCode(String code) {
		this.code = code;
	}
	
}
