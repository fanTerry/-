/*border*/
.border{position: relative;}
.border:before{
    content: " "!important;
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
	border: 0 solid #ddd;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    box-sizing: border-box;
    pointer-events: none;
    background: transparent;
    z-index: 1;
}

/*border-all*/
.bda:before{ border-width: 1px; }

/*border-top*/
.bdt:before{ border-top-width: 1px; }

/*border-right*/
.bdr:before{ border-right-width: 1px; }

/*border-bottom*/
